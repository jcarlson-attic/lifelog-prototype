<h1>New entry</h1>

<%= error_messages_for :entry %>

<% form_for(@entry) do |f| %>
  <p>
    <b>Id</b><br />
    <%= f.text_field :id %>
  </p>

  <p>
    <b>Entry type</b><br />
    <%= f.text_field :entry_type_id %>
  </p>

  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', entries_path %>
