<h1>Available Entry Types</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Complex</th>
  </tr>

<% for entry_type in @entry_types %>
  <tr>
    <td><%=h entry_type.name %></td>
    <td><%=h (entry_type.complex ? 'Y' : 'N') %></td>
    <td><%= link_to 'Show', entry_type %></td>
    <td><%= link_to 'Edit', edit_entry_type_path(entry_type) %></td>
    <td><%= link_to 'Destroy', entry_type, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Entry Type', new_entry_type_path %><br />
<%= link_to 'Jump to Attribute Types', attribute_types_path %><br />
<%= link_to 'Add Attribute Type Association', new_entry_type_attribute_type_path %>
